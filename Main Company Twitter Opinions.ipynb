    "        output_file = f'output/twitter_tweets_{timestamp}.csv'\n",
    "        all_tweets_df.to_csv(output_file, index=False, encoding='utf-8')\n",
    "        logger.info(f\"Tweets gemt i {output_file}\")\n",
    "\n",
    "        return all_tweets_df\n",
