    "                    })\n",
    "            except Exception as e:\n",
    "                logger.error(f\"Fejl under s√∏gning for '{keyword}': {e}\")\n",
    "                continue\n",
    "\n",
    "        # Konverter til DataFrame og fjern duplikater\n",
    "        all_tweets_df = pd.DataFrame(all_tweets)\n",
