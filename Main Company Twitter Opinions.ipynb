    "            'Search_keyword': keyword\n",
    "            \n",
    "        }\n",
    "        tweet_data.append(tweet_info)\n",
    "    \n",
    "    return pd.DataFrame(tweet_data)\n",
    "async def main():\n",
    "    try:\n",
    "        # Initialis√©r klienten\n",
    "        client = Client(language='en-US')\n",
    "        logger.info(\"Klient initialiseret.\")\n",
    "        \n",
    "        # Indl√¶s cookies fra fil\n",
    "        try:\n",
    "            with open(COOKIES_FILE, 'r', encoding='utf-8') as f:\n",
    "                cookies = json.load(f)\n",
    "            formatted_cookies = format_cookies_for_httpx(cookies)\n",
    "            client.set_cookies(formatted_cookies)\n",
    "            logger.info(\"Cookies indl√¶st og appliceret.\")\n",
    "        except Exception as e:\n",
    "            logger.error(f\"Fejl ved indl√¶sning af cookies: {e}\")\n",
    "            return None\n",
    "\n",
    "        all_tweets = []\n",
    "\n",
    "        for keyword in keywords:\n",
    "            logger.info(f\"S√∏ger efter tweets for '{keyword}'...\")\n",
    "            tweets = []\n",
    "            try:\n",
    "                # Start med en initial s√∏gning\n",
    "                results = await client.search_tweet(\n",
    "                    query=keyword,\n",
    "                    product='Top',\n",
    "                    count=20  # Maks antal pr. foresp√∏rgsel\n",
    "                )\n",
    "\n",
    "                # Tilf√∏j initiale tweets\n",
    "                tweets.extend(results)\n",
    "\n",
    "                # Forts√¶t med at hente, indtil vi n√•r gr√¶nsen eller l√∏ber t√∏r for tweets\n",
    "                while len(tweets) < tweets_per_keyword and results.next_cursor:\n",
    "                    logger.info(f\"Henter flere tweets for '{keyword}'...\")\n",
    "                    try:\n",
    "                        # Hent n√¶ste batch af tweets\n",
    "                        results = await results.next()\n",
    "\n",
    "                        # Tilf√∏j tweets fra n√¶ste batch\n",
    "                        tweets.extend(results)\n",
    "\n",
    "                        # Pause for at undg√• rate limits\n",
    "                        await asyncio.sleep(0.5)\n",
    "                    except Exception as next_error:\n",
    "                        logger.warning(f\"Fejl ved hentning af flere tweets: {next_error}\")\n",
    "                        break\n",
    "\n",
    "                # Log resultatet for det p√•g√¶ldende keyword\n",
    "                logger.info(f\"Fandt {len(tweets)} tweets for '{keyword}'.\")\n",
    "\n",
    "                # Tilf√∏j tweet data til all_tweets\n",
    "                for tweet in tweets:\n",
    "                    all_tweets.append({\n",
    "                        'Tweet_id': tweet.id,\n",
    "                        'User_name': tweet.user.name,\n",
    "                        'Screen_name': tweet.user.screen_name,\n",
